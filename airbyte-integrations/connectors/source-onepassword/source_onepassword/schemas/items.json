{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "description": "Unique identifier for the item"
    },
    "title": {
      "type": "string",
      "description": "Title of the item"
    },
    "category": {
      "type": "string",
      "description": "Category of the item (Login, Secure Note, etc.)"
    },
    "vault_id": {
      "type": "string",
      "description": "ID of the vault containing this item"
    },
    "vault_name": {
      "type": "string",
      "description": "Name of the vault containing this item"
    },
    "created_at": {
      "type": "string",
      "format": "date-time",
      "description": "Item creation timestamp"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time",
      "description": "Last update timestamp"
    },
    "favorite": {
      "type": "boolean",
      "description": "Whether the item is marked as favorite"
    },
    "tags": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "description": "Tags associated with the item"
    },
    "urls": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "href": {
            "type": "string"
          },
          "primary": {
            "type": "boolean"
          }
        }
      },
      "description": "URLs associated with the item"
    }
  }
}