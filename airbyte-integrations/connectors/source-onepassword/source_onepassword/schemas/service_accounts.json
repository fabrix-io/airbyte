{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "description": "Unique identifier for the service account"
    },
    "name": {
      "type": "string",
      "description": "Name of the service account"
    },
    "email": {
      "type": "string",
      "format": "email",
      "description": "Email associated with the service account"
    },
    "state": {
      "type": "string",
      "description": "Current state of the service account"
    },
    "created_at": {
      "type": "string",
      "format": "date-time",
      "description": "Service account creation timestamp"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time",
      "description": "Last update timestamp"
    }
  }
}